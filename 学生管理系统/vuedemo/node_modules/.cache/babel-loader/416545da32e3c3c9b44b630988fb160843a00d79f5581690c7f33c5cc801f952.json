 />\r\n        <el-table-column prop=\"name\" label=\"姓名\" />\r\n        <el-table-column prop=\"age\" label=\"年龄\" />\r\n        <el-table-column prop=\"grade\" label=\"年级\" />\r\n        <el-table-column prop=\"clazz\" label=\"班级\" />\r\n        <el-table-column label=\"操作\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button type=\"text\" @click=\"editStudent(scope.row)\"\r\n              >编辑</el-button\r\n            >\r\n            <el-button type=\"text\" @click=\"removeStudent(scope.row)\"\r\n              >删除</el-button\r\n            >\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n\r\n      <!--开启分页-->\r\n      <el-pagination\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n        :current-page=\"currentPage\"\r\n        :page-sizes=\"[10, 20, 30, 40]\"\r\n        :page-size=\"pageSize\"\r\n        layout=\"total, sizes, prev, pager, next, jumper\"\r\n        :total=\"students.length\"\r\n        prev-text=\"上一页\"\r\n        next-text=\"下一页\"\r\n        background\r\n      >\r\n      </el-pagination>\r\n    </el-card>\r\n\r\n    <!-- 添加学生对话框 -->\r\n    <el-dialog\r\n      title=\"添加学生\"\r\n      :visible.sync=\"addStudentDialogVisible\"\r\n      width=\"30%\"\r\n      @close=\"resetForm('addStudentForm')\"\r\n    >\r\n      <el-form ref=\"addStudentForm\" :model=\"addStudentForm\" label-width=\"80px\">\r\n        <el-form-item label=\"姓名\" prop=\"name\">\r\n          <el-input v-model=\"addStudentForm.name\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"年龄\" prop=\"age\">\r\n          <el-input v-model=\"addStudentForm.age\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"年级\" prop=\"grade\">\r\n          <el-input v-model=\"addStudentForm.grade\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"班级\" prop=\"clazz\">\r\n          <el-input v-model=\"addStudentForm.clazz\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"addStudentDialogVisible = false\">取消</el-button>\r\n        <el-button type=\"primary\" @click=\"addStudent\">确认添加</el-button>\r\n      </span>\r\n    </el-dialog>\r\n\r\n    <!-- 编辑学生对话框 -->\r\n    <el-dialog\r\n      title=\"编辑学生信息\"\r\n      :visible.sync=\"editStudentDialogVisible\"\r\n      width=\"30%\"\r\n      @close=\"resetForm('editStudentForm')\"\r\n    >\r\n      <el-form\r\n        ref=\"editStudentForm\"\r\n        :model=\"editStudentForm\"\r\n        label-width=\"80px\"\r\n      >\r\n        <el-form-item label=\"姓名\" prop=\"name\">\r\n          <el-input v-model=\"editStudentForm.name\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"年龄\" prop=\"age\">\r\n          <el-input v-model=\"editStudentForm.age\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"年级\" prop=\"grade\">\r\n          <el-input v-model=\"editStudentForm.grade\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"班级\" prop=\"clazz\">\r\n          <el-input v-model=\"editStudentForm.clazz\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <span slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"editStudentDialogVisible = false\">取消</el-button>\r\n        <el-button type=\"primary\" @click=\"updateStudent\">确认修改</el-button>\r\n      </span>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"VuedemoHome\",\r\n\r\n  data() {\r\n    return {\r\n      students: [],\r\n      addStudentDialogVisible: false,\r\n      addStudentForm: {\r\n        name: \"\",\r\n        age: \"\",\r\n        grade: \"\",\r\n        clazz: \"\",\r\n      },\r\n      editStudentDialogVisible: false,\r\n      editStudentForm: {\r\n        id: \"\",\r\n        name: \"\",\r\n        age: \"\",\r\n        grade: \"\",\r\n        clazz: \"\",\r\n      },\r\n      currentPage: 1,\r\n      pageSize: 10,\r\n    };\r\n  },\r\n  computed: {\r\n    displayStudents() {\r\n      const start = (this.currentPage - 1) * this.pageSize;\r\n      const end = start + this.pageSize;\r\n      return this.students.slice(start, end);\r\n    },\r\n  },\r\n  created() {\r\n    this.fetchStudents();\r\n  },\r\n\r\n  methods: {\r\n    fetchStudents() {\r\n      this.$axios.get(\"/getAll\").then((res) => {\r\n        this.students = res.data;\r\n      });\r\n    },\r\n    resetForm(formName) {\r\n      this.$refs[formName].resetFields();\r\n    },\r\n    addStudent() {\r\n      this.$axios\r\n        .post(\"/add\", this.addStudentForm)\r\n        .then((res) => {\r\n          if (res.data === \"success\") {\r\n            this.addStudentDialogVisible = false;\r\n            this.resetForm(\"addStudentsForm\");\r\n            this.fetchStudents();\r\n            this.$message.success(\"添加成功\");\r\n          } else {\r\n            this.$message.error(\"添加失败\");\r\n          }\r\n        })\r\n        .catch((error) => {\r\n          this.$message.error(\"添加失败\");\r\n          console.error(error);\r\n        });\r\n    },\r\n    editStudent(student) {\r\n      this.editStudentForm = {\r\n        id: student.id,\r\n        name: student.name,\r\n        age: student.age,\r\n        grade: student.grade,\r\n        clazz: student.clazz,\r\n      };\r\n      this.editStudentDialogVisible = true;\r\n    },\r\n    updateStudent() {\r\n      this.$axios.post(\"/update\", this.editStudentForm).then(() => {\r\n        this.editStudentDialogVisible = false;\r\n        this.resetForm(\"editStudentForm\");\r\n        this.fetchStudents();\r\n      });\r\n    },\r\n    removeStudent(student) {\r\n      this.$axios.post(\"/delete\", { id: student.id }).then(() => {\r\n        this.fetchStudents();\r\n      });\r\n    },\r\n    handleCurrentChange(val) {\r\n      this.currentPage = val;\r\n    },\r\n    handleSizeChange(val) {\r\n      this.pageSize = val;\r\n    },\r\n    logout() {\r\n      this.$router.push(\"/login\");\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.home {\r\n  padding: 10px;\r\n}\r\n.home-text {\r\n  font-size: 30px;\r\n}\r\n.home-button {\r\n  margin-top: 10px;\r\n}\r\n\r\n.el-pagination {\r\n  margin-top: 20px;\r\n  text-align: center;\r\n  font-size: 18px;\r\n}\r\n</style>\r\n"],"mappings":";AAyHA;EACAA,IAAA;EAEAC,KAAA;IACA;MACAC,QAAA;MACAC,uBAAA;MACAC,cAAA;QACAJ,IAAA;QACAK,GAAA;QACAC,KAAA;QACAC,KAAA;MACA;MACAC,wBAAA;MACAC,eAAA;QACAC,EAAA;QACAV,IAAA;QACAK,GAAA;QACAC,KAAA;QACAC,KAAA;MACA;MACAI,WAAA;MACAC,QAAA;IACA;EACA;EACAC,QAAA;IACAC,gBAAA;MACA,MAAAC,KAAA,SAAAJ,WAAA,aAAAC,QAAA;MACA,MAAAI,GAAA,GAAAD,KAAA,QAAAH,QAAA;MACA,YAAAV,QAAA,CAAAe,KAAA,CAAAF,KAAA,EAAAC,GAAA;IACA;EACA;EACAE,QAAA;IACA,KAAAC,aAAA;EACA;EAEAC,OAAA;IACAD,cAAA;MACA,KAAAE,MAAA,CAAAC,GAAA,YAAAC,IAAA,CAAAC,GAAA;QACA,KAAAtB,QAAA,GAAAsB,GAAA,CAAAvB,IAAA;MACA;IACA;IACAwB,UAAAC,QAAA;MACA,KAAAC,KAAA,CAAAD,QAAA,EAAAE,WAAA;IACA;IACAC,WAAA;MACA,KAAAR,MAAA,CACAS,IAAA,cAAA1B,cAAA,EACAmB,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAvB,IAAA;UACA,KAAAE,uBAAA;UACA,KAAAsB,SAAA;UACA,KAAAN,aAAA;UACA,KAAAY,QAAA,CAAAC,OAAA;QACA;UACA,KAAAD,QAAA,CAAAE,KAAA;QACA;MACA,GACAC,KAAA,CAAAD,KAAA;QACA,KAAAF,QAAA,CAAAE,KAAA;QACAE,OAAA,CAAAF,KAAA,CAAAA,KAAA;MACA;IACA;IACAG,YAAAC,OAAA;MACA,KAAA5B,eAAA;QACAC,EAAA,EAAA2B,OAAA,CAAA3B,EAAA;QACAV,IAAA,EAAAqC,OAAA,CAAArC,IAAA;QACAK,GAAA,EAAAgC,OAAA,CAAAhC,GAAA;QACAC,KAAA,EAAA+B,OAAA,CAAA/B,KAAA;QACAC,KAAA,EAAA8B,OAAA,CAAA9B;MACA;MACA,KAAAC,wBAAA;IACA;IACA8B,cAAA;MACA,KAAAjB,MAAA,CAAAS,IAAA,iBAAArB,eAAA,EAAAc,IAAA;QACA,KAAAf,wBAAA;QACA,KAAAiB,SAAA;QACA,KAAAN,aAAA;MACA;IACA;IACAoB,cAAAF,OAAA;MACA,KAAAhB,MAAA,CAAAS,IAAA;QAAApB,EAAA,EAAA2B,OAAA,CAAA3B;MAAA,GAAAa,IAAA;QACA,KAAAJ,aAAA;MACA;IACA;IACAqB,oBAAAC,GAAA;MACA,KAAA9B,WAAA,GAAA8B,GAAA;IACA;IACAC,iBAAAD,GAAA;MACA,KAAA7B,QAAA,GAAA6B,GAAA;IACA;IACAE,OAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDe