{"ast":null,"code":"export default {\n  name: \"VuedemoHome\",\n  data() {\n    return {};\n  },\n  mounted() {},\n  methods: {}\n};","map":{"version":3,"names":["name","data","mounted","methods"],"sources":["src/components/Home.vue"],"sourcesContent":["<template>\r\n  <div class=\"home\">\r\n    <el-card>\r\n      <!-- 头部 -->\r\n      <el-header>\r\n        <el-row type=\"flex\" justify=\"space-around\">\r\n          <el-col :span=\"8\" style=\"text-align: center\">\r\n            <el-button type=\"primary\" class=\"home-button\">添加学生</el-button>\r\n          </el-col>\r\n          <el-col :span=\"8\" style=\"text-align: center\">\r\n            <div class=\"home-text\">学生管理系统</div>\r\n          </el-col>\r\n          <el-col :span=\"8\" style=\"text-align: center\">\r\n            <el-button type=\"primary\" class=\"home-button\">退出系统</el-button>\r\n          </el-col>\r\n        </el-row>\r\n      </el-header>\r\n\r\n      <!-- 主体 -->\r\n      <el-table :data=\"tableData\" border style=\"width: 100%\">\r\n        <el-table-column prop=\"id\" label=\"ID\" />\r\n        <el-table-column prop=\"name\" label=\"姓名\" />\r\n        <el-table-column prop=\"age\" label=\"年龄\" />\r\n        <el-table-column prop=\"grade\" label=\"年级\" />\r\n        <el-table-column prop=\"clazz\" label=\"班级\" />\r\n        <el-table-column label=\"操作\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button type=\"text\" @click=\"editStudent(scope.row)\"\r\n              >编辑</el-button\r\n            >\r\n            <el-button type=\"text\" @click=\"removeStudent(scope.row)\"\r\n              >删除</el-button\r\n            >\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n\r\n      <!--开启分页-->\r\n      <el-pagination\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n        :current-page=\"currentPage\"\r\n        :page-sizes=\"[10, 20, 30, 40]\"\r\n        :page-size=\"pageSize\"\r\n        layout=\"total, sizes, prev, pager, next, jumper\"\r\n        :total=\"students.length\"\r\n        prev-text=\"上一页\"\r\n        next-text=\"下一页\"\r\n        background\r\n      >\r\n      </el-pagination>\r\n    </el-card>\r\n  </div>\r\n\r\n  <!-- 添加学生对话框 -->\r\n  <el-dialog\r\n    title=\"添加学生\"\r\n    v-model=\"addStudentDialogVisible\"\r\n    width=\"30%\"\r\n    @close=\"resetForm('addStudentForm')\"\r\n  >\r\n    <el-form ref=\"addStudentForm\" :model=\"addStudentForm\" label-width=\"80px\">\r\n      <el-form-item label=\"姓名\" prop=\"name\">\r\n        <el-input v-model=\"addStudentForm.name\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"年龄\" prop=\"age\">\r\n        <el-input v-model=\"addStudentForm.age\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"年级\" prop=\"grade\">\r\n        <el-input v-model=\"addStudentForm.grade\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"班级\" prop=\"clazz\">\r\n        <el-input v-model=\"addStudentForm.clazz\"></el-input>\r\n      </el-form-item>\r\n    </el-form>\r\n    <span slot=\"footer\" class=\"dialog-footer\">\r\n      <el-button @click=\"addStudentDialogVisible = false\">取消</el-button>\r\n      <el-button type=\"primary\" @click=\"addStudent\">确认添加</el-button>\r\n    </span>\r\n  </el-dialog>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"VuedemoHome\",\r\n\r\n  data() {\r\n    return {};\r\n  },\r\n\r\n  mounted() {},\r\n\r\n  methods: {},\r\n};\r\n</script>\r\n\r\n<style>\r\n.home {\r\n  padding: 20px;\r\n}\r\n.home-text {\r\n  font-size: 40px;\r\n}\r\n.home-button {\r\n  margin-top: 10px;\r\n}\r\n</style>\r\n"],"mappings":"AAmFA;EACAA,IAAA;EAEAC,KAAA;IACA;EACA;EAEAC,QAAA;EAEAC,OAAA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}